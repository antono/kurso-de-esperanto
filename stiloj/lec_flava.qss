

 QTabWidget::pane { /* The tab widget frame */ 
	 background-color: white;
 	 border-left: 1px solid #C4BEB0;
	 border-bottom: 1px solid #C1C1C1;
	 border-right: 0px;
	 border-top: 0px;}

 QTabBar::tab {
	height: 0px;
	width:  0px;
 }

neklakebla {
     border: 0px;
     min-width: 10px;
	 min-height: 1.2em;
	 max-height: 1.5em;
	 margin-right: 6px;
	 font-weight: bold;
	 color: #006AB9;
 }

 klakbutono {
     border: 0px;
     min-width: 20px;
	 /* min-height: 9ex; */
	 
	 margin: 5px;
	 font-weight: bold;
	 text-align: left; 
	 color: #006AB9;
	 padding: 2px;

 
 }
 
 klakbutono:hover {
   	 background: qlineargradient( x1:0, y1:0.2, x2:0, y2:1, stop:0 transparent, stop:0.89 transparent, stop:0.9 #006AB9, stop:1 #006AB9); 
}

klakbutono:pressed {
    padding-left: 4px;
	padding-top: 4px;
}

 panelo {
